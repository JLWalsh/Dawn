import lib
import check

@test
shouldGiveTen() {
    val value = lib.giveTen()

    check.isEqualTo(value, 10i)
}

@test
shouldDivideByTwo() {
    val numberToDivide = 20i

    val dividedValue = lib.divideByTwo(numberToDivide)

    check.isTrue(dividedValue == 10i)
}

@test
givenFalseBaseVariable_ImplicationShouldBeTrue() {
    val implication = lib.imply(false, true)

    check.isTrue(implication)
}
